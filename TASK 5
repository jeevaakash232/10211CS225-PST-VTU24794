int count = 0;

    for (int d = input1; d <= input2; d++)
    {
        if (d < 2)
            continue;

        int c = 0;
        for (int i = 2; i <= d / 2; i++)
        {
            if (d % i == 0)
            {
                c = 1;
                break;
            }
        }

        if (c == 0)
            count++;
    }
    return count;
